{
  "compilerOptions": {
    "baseUrl": "./src",
    "rootDir": "./src",
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es2015",
    "jsx": "react",
    "noImplicitAny": false,
    "sourceMap": false,
    "declaration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "declarationDir": "./dist/typings"
  },
  "exclude": ["dist", "node_modules"]
}
